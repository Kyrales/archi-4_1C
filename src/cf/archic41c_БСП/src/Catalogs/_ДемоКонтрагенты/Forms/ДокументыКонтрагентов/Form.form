    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	_ДемоРеализацияТоваровТовары.Ссылка,
	СУММА(_ДемоРеализацияТоваровТовары.Цена * _ДемоРеализацияТоваровТовары.Количество) КАК Сумма
ПОМЕСТИТЬ втСумма
ИЗ
	Документ._ДемоРеализацияТоваров.Товары КАК _ДемоРеализацияТоваровТовары
СГРУППИРОВАТЬ ПО
	_ДемоРеализацияТоваровТовары.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	_ДемоРеализацияТоваров.Ссылка,
	_ДемоРеализацияТоваров.Контрагент,
	NULL КАК ДокументКонтрагента,
	_ДемоРеализацияТоваров.Дата,
	_ДемоРеализацияТоваров.Номер,
	втСумма.Сумма,
	_ДемоРеализацияТоваров.Валюта
ИЗ
	Документ._ДемоРеализацияТоваров КАК _ДемоРеализацияТоваров
		ЛЕВОЕ СОЕДИНЕНИЕ втСумма КАК втСумма
		по _ДемоРеализацияТоваров.Ссылка = втСумма.Ссылка</queryText>
